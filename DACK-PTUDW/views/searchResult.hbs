<div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-body">
                        <table id="bootstrap-data-table-export" class="table table-striped table-bordered">

                            <thead>
                                <th>Name</th>
                                <th>NPC</th>
                                <th>Price</th>
                                <th>Image</th>
                            </thead>
                            <tbody>
                                {{#each product}}
                                <tr>
                                    <td><a href="" id={{id}}>{{name}}</a></td>
                                    <td>{{npc}}</td>
                                    <td>{{price}}</td>
                                    <td>
                                        <img src={{imgPath}} alt="" width="150px" height="150px" />
                                    </td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
{{#each product}}
    <script>
        var temp = "#" + "{{id}}";
        $(temp).attr("href","/product?id="+"{{id}}");
    </script>
{{/each}}
<script>

  $(id1).attr("href", "/searchResult?search="+data[0]+"&type=category");
  $(id2).attr("href", "/searchResult?search="+data[1]+"&type=category");
  $(id3).attr("href", "/searchResult?search="+data[2]+"&type=category");
</script>